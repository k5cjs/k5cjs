{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "rxjs": {
      "projectType": "library",
      "root": "projects/rxjs",
      "sourceRoot": "projects/rxjs/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rxjs/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rxjs/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rxjs/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rxjs/src/test.ts",
            "tsConfig": "projects/rxjs/tsconfig.spec.json",
            "karmaConfig": "projects/rxjs/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/rxjs/**/*.ts", "projects/rxjs/**/*.html"]
          }
        }
      }
    },
    "forms": {
      "projectType": "library",
      "root": "projects/forms",
      "sourceRoot": "projects/forms/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/forms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/forms/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/forms/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/forms/src/test.ts",
            "tsConfig": "projects/forms/tsconfig.spec.json",
            "karmaConfig": "projects/forms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/forms/**/*.ts", "projects/forms/**/*.html"]
          }
        }
      }
    },
    "tables": {
      "projectType": "library",
      "root": "projects/tables",
      "sourceRoot": "projects/tables/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/tables/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/tables/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/tables/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/tables/src/test.ts",
            "tsConfig": "projects/tables/tsconfig.spec.json",
            "karmaConfig": "projects/tables/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/tables/**/*.ts", "projects/tables/**/*.html"]
          }
        }
      }
    },
    "examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/examples",
      "sourceRoot": "projects/examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/examples",
            "index": "projects/examples/src/index.html",
            "main": "projects/examples/src/main.ts",
            "polyfills": "projects/examples/src/polyfills.ts",
            "tsConfig": "projects/examples/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/examples/src/favicon.ico", "projects/examples/src/assets"],
            "styles": ["projects/examples/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/examples/src/environments/environment.ts",
                  "with": "projects/examples/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "examples:build:production"
            },
            "development": {
              "browserTarget": "examples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "examples:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/examples/src/test.ts",
            "polyfills": "projects/examples/src/polyfills.ts",
            "tsConfig": "projects/examples/tsconfig.spec.json",
            "karmaConfig": "projects/examples/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/examples/src/favicon.ico", "projects/examples/src/assets"],
            "styles": ["projects/examples/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/examples/**/*.ts", "projects/examples/**/*.html"]
          }
        }
      }
    },
    "scroll": {
      "projectType": "library",
      "root": "projects/scroll",
      "sourceRoot": "projects/scroll/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/scroll/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/scroll/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/scroll/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/scroll/src/test.ts",
            "tsConfig": "projects/scroll/tsconfig.spec.json",
            "karmaConfig": "projects/scroll/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/scroll/**/*.ts", "projects/scroll/**/*.html"]
          }
        }
      }
    },
    "select": {
      "projectType": "library",
      "root": "projects/select",
      "sourceRoot": "projects/select/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/select/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/select/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/select/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/select/src/test.ts",
            "tsConfig": "projects/select/tsconfig.spec.json",
            "karmaConfig": "projects/select/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/select/**/*.ts", "projects/select/**/*.html"]
          }
        }
      }
    },
    "selection-model": {
      "projectType": "library",
      "root": "projects/selection-model",
      "sourceRoot": "projects/selection-model/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/selection-model/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/selection-model/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/selection-model/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/selection-model/src/test.ts",
            "tsConfig": "projects/selection-model/tsconfig.spec.json",
            "karmaConfig": "projects/selection-model/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/selection-model/**/*.ts", "projects/selection-model/**/*.html"]
          }
        }
      }
    }
  },
  "defaultProject": "rxjs",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
