{
  "name": "k5cjs",
  "version": "0.0.2",
  "config": {
    "files_stylelint": "./projects/{*.scss,**/*.scss}"
  },
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --host 0.0.0.0 --port 4200",
    "build:libs": "node ci/build-libraries.mjs",
    "build:libs:prod": "node ci/build-libraries.mjs --production",
    "build:libs:clean": "run-s clear:dist build:libs",
    "build:docs": "ng build docs --configuration=production --base-href=/k5cjs/docs/",
    "format:prettier": "node ./ci/prettier.mjs",
    "format:prettier:fix": "npm run format:prettier -- --fix",
    "format:eslint": "node ./ci/lint.mjs",
    "format:eslint:fix": "npm run format:eslint -- --fix",
    "format:stylelint": "stylelint $npm_package_config_files_stylelint",
    "format:stylelint:fix": "npm run format:stylelint -- --fix",
    "format": "run-s format:prettier format:eslint format:stylelint",
    "format:fix": "run-s format:prettier:fix format:eslint:fix format:stylelint:fix",
    "test:rxjs": "ng test rxjs --browsers=Firefox --code-coverage",
    "test:forms": "ng test forms --browsers=Firefox --code-coverage",
    "test:tables": "ng test tables --browsers=Firefox --code-coverage",
    "test:scroll": "ng test scroll --browsers=Firefox --code-coverage",
    "test:select": "ng test select --browsers=Firefox --code-coverage",
    "test:store": "ng test store --browsers=Firefox --code-coverage",
    "test:selection-model": "ng test selection-model --browsers=Firefox --code-coverage",
    "test:docs": "ng test docs --browsers=Firefox --code-coverage",
    "tests": "ng test --no-watch --code-coverage --browsers=ChromeHeadlessCI",
    "tests:dev": "ng test --no-watch --browsers=Firefox --code-coverage",
    "merge:coverage": "./ci/merge-coverage.sh",
    "coverage:rxjs": "http-server ./coverage/rxjs -p 4200",
    "coverage:forms": "http-server ./coverage/forms -p 4200",
    "coverage:tables": "http-server ./coverage/tables -p 4200",
    "coverage:scroll": "http-server ./coverage/scroll -p 4200",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "postinstall": "patch-package"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.6",
    "@angular/cdk": "^18.0.6",
    "@angular/common": "^18.0.6",
    "@angular/compiler": "^18.0.6",
    "@angular/core": "^18.0.6",
    "@angular/forms": "^18.0.6",
    "@angular/platform-browser": "^18.0.6",
    "@angular/platform-browser-dynamic": "^18.0.6",
    "@angular/router": "^18.0.6",
    "@k5cjs/select": "file:projects/select",
    "@k5cjs/selection-model": "file:projects/selection-model",
    "@ngrx/effects": "^18.0.1",
    "@ngrx/entity": "^18.0.1",
    "@ngrx/operators": "^18.0.0",
    "@ngrx/store": "^18.0.1",
    "object-hash": "^3.0.0",
    "patch-package": "^8.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular-eslint/builder": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/schematics": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.0.6",
    "@ngrx/eslint-plugin": "^18.1.1",
    "@ngrx/store-devtools": "^18.0.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/object-hash": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-sort-export-all": "^1.2.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "http-server": "^14.1.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mermaid": "^10.2.4",
    "ng-packagr": "^18.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "postcss": "^8.4.24",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.4.5",
    "vitepress": "^1.0.0-rc.10",
    "vitepress-plugin-mermaid": "^2.0.11",
    "vitepress-plugin-tabs": "^0.3.0",
    "zx": "^5.1.0"
  }
}
